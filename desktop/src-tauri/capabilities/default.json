{
  "$schema": "https://schema.tauri.app/capability/1",
  "identifier": "default",
  "description": "Default capability for AutoDoc Agent",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-set-title",
    "core:window:allow-close",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:default",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "fs:allow-read-dir",
    "fs:allow-create",
    "fs:allow-mkdir",
    "fs:allow-exists",
    "fs:scope-app-data",
    "fs:scope-app-config",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-open"
  ]
}
